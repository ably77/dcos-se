{
  "id": "/dev/tools/gitlab",
  "acceptedResourceRoles": [
    "*"
  ],
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "constraints": [
    [
      "hostname",
      "CLUSTER",
      "$PINNEDNODE"
    ]
  ],
  "container": {
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/gitlab:80"
        },
        "protocol": "tcp",
        "servicePort": 10103,
        "name": "web"
      },
      {
        "containerPort": 443,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 10104
      },
      {
        "containerPort": 22,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 22222
      },
      {
        "containerPort": 50000,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 50000
      }
    ],
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/var/opt/gitlab",
        "hostPath": "/srv/gitlab/gitlab/opt",
        "mode": "RW"
      },
      {
        "containerPath": "/var/log/gitlab",
        "hostPath": "/srv/gitlab/gitlab/log",
        "mode": "RW"
      },
      {
        "containerPath": "/etc/gitlab",
        "hostPath": "/srv/gitlab-data/gitlab/config",
        "mode": "RW"
      },
      {
        "containerPath": "/gitlab-data",
        "hostPath": "/srv/gitlab-data/gitlab/data",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "gitlab/gitlab-ce:9.1.0-ce.0",
      "forcePullImage": false,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 0,
  "env": {
    "GITLAB_OMNIBUS_CONFIG": "external_url 'http://gitlab.dcosappstudio.mesosphere.io/'; registry_external_url 'http://gitlab.dcosappstudio.mesosphere.io:50000/'; gitlab_rails['gitlab_ssh_host'] = 'gitlab.dcosappstudio.mesosphere.io:22222';  unicorn['worker_processes'] = 2; manage_accounts['enable'] = true; user['home'] = '/gitlab-data/home'; git_data_dir '/gitlab-data/git-data'; gitlab_rails['shared_path'] = '/gitlab-data/shared'; gitlab_rails['uploads_directory'] = '/gitlab-data/uploads'; gitlab_ci['builds_directory'] = '/gitlab-data/builds';"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 5,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "COMMAND",
      "command": {
        "value": "/opt/gitlab/bin/gitlab-healthcheck --fail > /dev/null"
      }
    }
  ],
  "instances": 1,
  "labels": {
    "DCOS_PACKAGE_OPTIONS": "",
    "HAPROXY_0_REDIRECT_TO_HTTPS": "false",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "HAPROXY_GROUP": "external",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_SERVICE_NAME": "gitlab",
    "DCOS_PACKAGE_DEFINITION": "",
    "DCOS_PACKAGE_VERSION": "1.0-9.1.0",
    "DCOS_PACKAGE_NAME": "gitlab",
    "MARATHON_SINGLE_INSTANCE_APP": "true",
    "HAPROXY_0_VHOST": "gitlab.dcosappstudio.mesosphere.io"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 2048,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "fetch": []
}