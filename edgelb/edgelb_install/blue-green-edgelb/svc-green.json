{
  "id": "/svc-green",
  "instances": 3,
  "cpus": 0.3,
  "mem": 1024,
  "cmd": "sed -i 's:Welcome to nginx!:This is svc-green:g' /usr/share/nginx/html/index.html; nginx -g 'daemon off;'",
  "container": {
    "portMappings": [{
      "containerPort": 80,
      "hostPort": 0,
      "protocol": "tcp",
      "servicePort": 0,
      "name": "svc-green"
    }],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "nginx"
    }
  },
  "networks": [{
    "mode": "container/bridge"
  }]
} 

